				float2 texCoord31 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float2 texCoord29 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float2 panner32 = ( 1.0 * _Time.y * float2( -1.5,0 ) + texCoord29);
				float simpleNoise30 = SimpleNoise( panner32*36.0 );
				float2 temp_cast_1 = (simpleNoise30).xx;
				float2 lerpResult40 = lerp( texCoord31 , temp_cast_1 , 0.585);
				float clampResult34 = clamp( texCoord29.x , 0.0 , 1.0 );
				float2 lerpResult36 = lerp( texCoord31 , lerpResult40 , pow( clampResult34 , 1.0 ));
				float temp_output_49_0 = ( ( (_remapminnoise + (tex2D( _noise, lerpResult45 ).r - 0.0) * (1.0 - _remapminnoise) / (1.0 - 0.0)) * pow( clampResult17 , _powercola ) * tex2D( _trail, lerpResult36 ).r ) * _destructor );
				float temp_output_51_0 = ( floor( temp_output_49_0 ) / _destructor );
				float2 temp_cast_2 = (pow( temp_output_51_0 , _powercolor )).xx;
				
				float clampResult21 = clamp( ( pow( temp_output_51_0 , _poweropac ) * 1.0 ) , 0.0 , 1.0 );
				
